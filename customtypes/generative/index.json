{
  "format": "page",
  "id": "generative",
  "label": "Generative",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "Generative title"
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Subtitle",
          "placeholder": "Short generative description"
        }
      },
      "prompt": {
        "type": "StructuredText",
        "config": {
          "label": "Prompt",
          "placeholder": "Generative prompt",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "images": {
        "type": "Group",
        "config": {
          "label": "Images",
          "repeat": true,
          "fields": {
            "image": {
              "type": "Image",
              "config": {
                "label": "Image",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      },
      "tool": {
        "type": "Select",
        "config": {
          "label": "Tool",
          "placeholder": "Generative tool",
          "options": [
            "Midjourney",
            "Ideogram",
            "Hailuo",
            "Krea",
            "Flux",
            "GPT"
          ],
          "default_value": "Midjourney"
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "Generative category",
          "options": [
            "Wallpaper & Background",
            "Interior & Architecture ",
            "Digital Illustration",
            "3d Render ",
            "2d Anime ",
            "Realistic Photography ",
            "Mockup"
          ]
        }
      },
      "highlight": {
        "type": "Boolean",
        "config": {
          "label": "Highlight",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "hide_prompt": {
        "type": "Boolean",
        "config": {
          "label": "Hide Prompt",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
